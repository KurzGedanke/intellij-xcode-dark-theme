<idea-plugin>
  <id>net.antelle.intellij-xcode-dark-theme</id>
  <name>XCode-Dark Theme</name>
  <version>1.0</version>
  <vendor email="antelle.net@gmail.com" url="https://antelle.net">Antelle</vendor>

  <description><![CDATA[
  This theme brings colors from the XCode Dark theme to IntelliJ.

  <br/><br/>
  To install:
  <ul>
  <li>Go to <i>Settings (Preferences) | Plugins</i>, find the theme plugin, and install it</li>
  <li>Restart the IDE</li>
  <li>Go to <i>Settings (Preferences) | Appearance & Behavior | Appearance</i> and select it in the <i>Theme</i> dropdown</li>
  </ul>
  <br/>

  <a href="https://github.com/antelle/intellij-xcode-dark-theme">GitHub: source code and issues</a>
  <br/><br/><br/>
  <img src="https://github.com/antelle/intellij-xcode-dark-theme/raw/master/images/screenshot-code.png" border="0" width="1005" alt="Code screenshot">
  <br/>
  <img src="https://raw.githubusercontent.com/antelle/intellij-xcode-dark-theme/master/images/screenshot-settings.png" border="0" width="1012" alt="Settings screenshot">
  <br/>
  <img src="https://raw.githubusercontent.com/antelle/intellij-xcode-dark-theme/master/images/screenshot-actions.png" border="0" width="718" alt="Actions screenshot">
  ]]></description>

  <change-notes><![CDATA[
    <p>v1.0.0 <small>2019-10-01</small></p><ul>
    <li>First version released</li>
    </ul>
  ]]></change-notes>

  <idea-version since-build="173.0"/>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="ef29b6ef-e306-45f0-919d-6264c9105248" path="/XCode-Dark.theme.json"/>
  </extensions>

  <actions>
  </actions>

</idea-plugin>
